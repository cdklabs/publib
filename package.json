    "eslint-plugin-import": "^2.31.0",
    "jest": "^27.5.1",
    "jest-junit": "^16",
    "projen": "^0.92.9",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.7"
  },
  "dependencies": {
    "@aws-sdk/client-codeartifact": "^3.826.0",
    "@aws-sdk/credential-providers": "^3.826.0",
    "@aws-sdk/types": "^3.821.0",
    "@types/fs-extra": "^8.0.0",
